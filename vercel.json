{
  "version": 2,
  "builds": [
     {
       "src": "manage.py",
       "use": "@vercel/python",
       "config": {
         "maxLambdaSize": "15mb",
         "runtime": "python3.9"
       }
     }
  ],
  "routes": [
     {
       "src": "/static/(.*)",
       "dest": "/staticfiles/$1"
     },
     {
       "src": "/(.*)",
       "dest": "manage.py"
     }
  ],
  "env": {
     "DJANGO_SETTINGS_MODULE": "myproject.settings"
  },
  "build": {
     "env": {
       "DJANGO_SETTINGS_MODULE": "myproject.settings"
     },
     "commands": [
       "pip install -r requirements.txt",
       "python manage.py collectstatic --noinput"
     ]
  }
 }
 